{
  "version": 2,
  "builds": [{ "src": "swagger-ui/**", "use": "@vercel/static" }],
  "routes": [
    {
      "src": "/openapi.json",
      "dest": "/openapi.json"
    },
    {
      "src": "/swagger-ui/(.*)",
      "dest": "/swagger-ui/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/swagger-ui/index.html"
    }
  ]
}
